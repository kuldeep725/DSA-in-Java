import java.util.Scanner;


public class ChocoLand {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-- >0) {
			int n = sc.nextInt();
			int k = sc.nextInt();
			int s = sc.nextInt();
			if (n * (s - s/7) < s *k) {
				System.out.println(-1);
			}
			else {
				if(s*k <= n)	{
					System.out.println(1);
				}
				else {
					int val = (s*k)/n;
					if((s*k) % n == 0) {
						System.out.println(val);
					}
					else {
						System.out.println(val+1);
					}
					
				}
			}
		}
		sc.close();
		
	}

}
