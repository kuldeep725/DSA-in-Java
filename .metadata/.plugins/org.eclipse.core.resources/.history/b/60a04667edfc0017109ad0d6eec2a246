		StringBuilder sb = new StringBuilder();
		if(a.length() < b.length()) {
			
			for (int i = 9; i >= 0; i--) {
				String key = String.valueOf(i);
				if(map.containsKey(key)) {
					map.put(key, map.get(key)-1);
//					System.out.print(key);
//					out += key;
					sb.append(key);
					if(map.get(key) == 0) 	map.remove(key);
					else						 i++;
				}
			}
		}
		else {
			int flag = 0;
			
			for(int i = 0; i < b.length(); i++) {
				String key = String.valueOf(b.charAt(i));
				int keyInt = Integer.parseInt(key);
				int flagNone = 0;
				
				while(keyInt >= 0 && flag == 0) {
					key = String.valueOf(keyInt);
					if(map.containsKey(key)) {
						map.put(key, map.get(key)-1);
//						System.out.print(key);
//						out += key;
						sb.append(key);
						if(Integer.parseInt(String.valueOf(b.charAt(i))) > keyInt) {
							flag = 1;
						}
						flagNone = 1;
						if(map.get(key) == 0) 	map.remove(key);
						break;
					}
					keyInt--;
				}
				if(flagNone == 0) {
					for(int j = i-1; j >= 0; j--) {
						sb.deleteCharAt(j);
						String keyFlag  = String.valueOf(a.charAt(j));
						int flagLoop  = 0;
						if(!map.containsKey(keyFlag)) {
//							System.out.println("if");
							map.put(keyFlag, 1);
						}
						else {
//							System.out.println("else");
							int value = map.get(keyFlag);
//							System.out.println("done");
							map.put(keyFlag, value+1);
						}
						for(int k = Integer.parseInt(keyFlag)-1; k >=0; k--) {
							keyFlag = String.valueOf(k);
							if(map.containsKey(keyFlag)) {
								sb.append(keyFlag);
								map.put(keyFlag, map.get(keyFlag)-1);
								if(map.get(keyFlag) == 0) 	map.remove(keyFlag);
								flagLoop = 1;
								break;
							}
						}
						if(flagLoop == 1) 	break;
					}
					flag = 1;
					break;
				}
				if(flag == 1) {
					break;
				}
				
			}
			if(flag == 1) {
				for (int j = 9; j>= 0; j--) {
				String key = String.valueOf(j);
					if(map.containsKey(key)) {
						map.put(key, map.get(key)-1);
//						System.out.print(key);
//						out += key;
						sb.append(key);
						if(map.get(key) == 0) 	map.remove(key);
						else						 j++;
					}
				}
			}
			System.out.println(sb.toString());
		}

